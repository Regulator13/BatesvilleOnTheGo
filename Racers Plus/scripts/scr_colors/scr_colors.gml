function get_html_color(index){
	switch(index){
		case 0:
			return "#ffffff"
		case 1:
			return "#0000ff"
		case 2:
			return "#ff0000"
		case 3:
			return "#33cc33"
		case 4:
			return "#cc0099"
		case 5:
			return "#00ffcc"
		case 6:
			return "#ff9900"
		case 7:
			return "#9900cc"
		case 8:
			return "#339933"
		case 9:
			return "#ffff00"
	}
}

